    "# Plot the distribution on histograms \n",
    "\n",
    "#set the histogram titles\n",
    "hist_titles = [\"Hour at which passengers were picked up\",\n",
    "              \"Number of passengers on board\"]\n",
    "\n",
    "#set the axis labels\n",
    "axis_labels = [\"Time /hour\",\n",
    "              \"Passenger /#\"]\n",
    "\n",
    "#create a base axis for the plots\n",
    "fig, ax = plt.subplots(1,2,figsize = (20,6))\n",
    "#flatten the axis to make it easy to iteratre over\n",
    "axis = ax.flatten()\n",
    "\n",
    "# discretise hour for histogram\n",
    "d = np.diff(np.unique(df_21wed.pickup_hour)).min()\n",
    "left_of_first_bin = df_21wed.pickup_hour.min() - float(d)/2\n",
    "right_of_last_bin = df_21wed.pickup_hour.max() + float(d)/2\n",
    "\n",
    "#create 2 histograms independently\n",
    "df_21wed[hist[0]].hist(bins=np.arange(left_of_first_bin, right_of_last_bin + d, d), ax = axis[0], color = \"blue\", alpha = 0.7)\n",
    "df_21wed[hist[1]].hist(bins = 5, ax = axis[1], color = \"blue\", alpha = 0.7)\n",
    "\n",
    "###iterate over each columns using the labels information already set up###\n",
    "for i, col in enumerate(hist):\n",
    "    #add label information\n",
    "    axis[i].set_title(hist_titles[i], fontsize = 25, pad = 25)\n",
    "    axis[i].set_ylabel(\"Frequency\", fontsize  =20, labelpad = 30)\n",
    "    axis[i].set_xlabel(f\"{axis_labels[i]}\", fontsize = 20, labelpad = 20)\n",
    "    axis[i].tick_params(axis = \"both\", labelsize = 20) \n",
    "\n",
    "#keep the layout tight\n",
    "plt.tight_layout()\n",
    "#show the plot \n",
    "plt.show()\n",
    "\n",
    "os.makedirs('output', exist_ok=True)\n",
