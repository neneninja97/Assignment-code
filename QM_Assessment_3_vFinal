    "dest = os.path.join(\"data\", \"raw\")\n",
    "\n",
    "# Relevant columns\n",
    "cols = ['PULocationID', 'DOLocationID', 'RatecodeID', 'fare_amount', 'extra', 'mta_tax', 'tip_amount', \n",
    "        'tolls_amount', 'improvement_surcharge', 'total_amount', 'trip_distance', \n",
    "        'tpep_pickup_datetime', 'passenger_count', 'payment_type']"
