    "### 4.3 Conducting K-means clustering"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 61,
   "id": "ee544e75-263a-4c20-b28b-043974a47472",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "KMeans(n_clusters=4, random_state=42)"
      ]
     },
     "execution_count": 61,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "#perform the clustering with the chosen number of clusters of 2\n",
    "kmeans_method = KMeans(n_clusters = 4,\n",
    "                       random_state = 42,\n",
    "                       n_init = 10)         # Take average of 10 iterations\n",
    "kmeans_method.fit(df_21wed[standardised_vars])"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 62,
   "id": "14d54b33-2382-4407-9b00-a092866fedf8",
   "metadata": {},
   "outputs": [],
   "source": [
    "#extract the cluster labels and add these back to the master data\n",
    "labels = kmeans_method.labels_\n",
    "df_21wed[\"labels\"] = labels\n",
    "df_21wed[\"labels\"] = df_21wed[\"labels\"] + 1 #adjusting the indexing to start at 1"
   ]
  },
  {
   "cell_type": "markdown",
   "id": "faf04fde-aac6-440a-b95e-7364baf6710b",
   "metadata": {},
   "source": [
    "### 4.4 Component analysis"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 63,
   "id": "3e120843-5f28-4d56-a8f3-f4cdac02e1b3",
   "metadata": {},
   "outputs": [],
   "source": [
    "# These are the components we want to investigate for each cluster\n",
    "comp = ['fare_amount', 'tip_amount', 'trip_distance', 'extra', \n",
    "        'pickup_hour','passenger_count']\n",
    "other = ['Payment type']"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 64,
   "id": "83da8cf8-b386-42b4-bdc3-dbeeb2ab1581",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "image/png": "\n",
      "text/plain": [
       "<Figure size 1440x720 with 6 Axes>"
      ]
     },
     "metadata": {
      "needs_background": "light"
     },
     "output_type": "display_data"
    }
   ],
   "source": [
    "#creating median plot\n",
    "#set the titles\n",
    "titles = ['Meter fare', 'Tip', 'Distance', 'Congestion/ overnight',\n",
    "               'Pick-up time', 'Passenger count']\n",
    "#set the labels\n",
    "ylabels = [\"Median price ($)\", \"Median price ($)\", \"Median distance(mile)\", \"Median price ($)\",\n",
    "           \"Median time (hour)\", \"Median passenger(#)\"]\n",
    "\n",
    "#create a base plot\n",
    "fig, ax = plt.subplots(2,3, figsize = (20,10))\n",
    "#flatten the axis\n",
    "axis = ax.flatten()\n",
    "\n",
    "#iterate over each column to create plots for each \n",
    "for i, col in enumerate(comp):\n",
    "    #create an empty dictionary\n",
    "    col_dict = {}\n",
    "    #iterate over each label\n",
    "    for label in list(df_21wed[\"labels\"].unique()):\n",
    "        #crete a new dataframe for each label\n",
    "        label_df = df_21wed[df_21wed[\"labels\"] == label]\n",
    "        #add the mean to the dataframe\n",
    "        col_dict[label] = label_df[col].median()\n",
    "    #convert the dictionary to a dataframe\n",
    "    column_df = pd.DataFrame.from_dict(col_dict, orient = \"index\")\n",
    "    #reset the index\n",
    "    column_df.reset_index(inplace=True)\n",
    "    #sort the values by the index\n",
    "    column_df.sort_values(by = \"index\", inplace=True)\n",
    "    \n",
    "    #plot the results\n",
    "    axis[i].plot(column_df[\"index\"], column_df[0],\n",
    "                marker = \"o\")\n",
    "    \n",
    "    #set the plots up\n",
    "    axis[i].set_title(titles[i], fontsize = 30, pad = 25)\n",
    "    axis[i].set_xlabel(\"Cluster ID\", fontsize = 20, labelpad = 25)\n",
    "    axis[i].xaxis.set_major_locator(MaxNLocator(integer=True))\n",
    "    axis[i].set_ylabel(ylabels[i], fontsize =20, labelpad = 25)\n",
    "    axis[i].tick_params(axis = \"both\", labelsize = 20)\n",
    "\n",
    "### Add median and median data (horizontal lines)\n",
    "axis[0].axhline(y = 8.5, color = \"red\", linestyle = \"--\")   # fare amount \n",
    "axis[1].axhline(y = 2.0, color = \"red\", linestyle = \"--\")   # tip amount\n",
    "axis[2].axhline(y = 1.53, color = \"red\", linestyle = \"--\")   # trip distance\n",
    "axis[3].axhline(y = 1.08, color = \"red\", linestyle = \"--\")   # extra surcharge (rush hour/ overnight)\n",
    "axis[4].axhline(y = 14, color = \"red\", linestyle = \"--\")   # pickup hour\n",
    "axis[5].axhline(y = 1, color = \"red\", linestyle = \"--\")   # passenger count\n",
    "    \n",
    "#set the layout to tight so no overalp\n",
    "plt.tight_layout()\n",
    "os.makedirs('output', exist_ok=True)\n",
    "fig.savefig(os.path.join('output','component.jpg'))\n",
    "\n",
    "### Code credit - Phillip Wilkinson (CASA)###"
   ]
  },
  {
