   ],
   "source": [
    "# Take log first to make the distribution of clustering variables more normal\n",
    "\n",
    "# For tips, there are some datapoints with tip = 0\n",
    "\n",
    "for col in take_log:\n",
    "    #create a new log column and convert the data\n",
    "    df_21wed[col + \"_log\"] = np.log(df_21wed[col])\n",
    "    \n",
    "# For tips, there are some datapoints with tip = 0\n",
    "# Convert tips_log of these variables articificially below all the other variables; -10\n",
    "df_21wed['tip_amount_log'] = df_21wed['tip_amount_log'].replace([np.inf, -np.inf], -1)\n"
   ]
  },
